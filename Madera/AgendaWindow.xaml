<Window x:Class="Madera.AgendaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Madera"
        mc:Ignorable="d"
        Title="AgendaWindow" Height="450" Width="800">
    <Grid>
        <Calendar x:Name="calendar" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Height="379" Width="306" SelectedDatesChanged="DateChanged"/>
        <Button x:Name="btnRetour" Content="Retour" HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top" Background="#FFB8B8B8" Click="btnRetour_Click"/>

    </Grid>
</Window>
