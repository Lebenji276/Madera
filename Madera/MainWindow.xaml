<Window x:Class="Madera.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Madera"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btn_valider" Content="Valider" HorizontalAlignment="Center" Margin="0,246,0,0" Click="OnClick1" VerticalAlignment="Top" ClickMode="Release"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Margin="0,177,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Center" VerticalAlignment="Center" Width="120"/>
        <Label x:Name="label_1" Content="Label" HorizontalAlignment="Left" Margin="148,362,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_2" Content="Label" HorizontalAlignment="Left" Margin="406,362,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btn_valider_Copy" Content="Valider" HorizontalAlignment="Left" Margin="518,246,0,0" Click="OnClick1" VerticalAlignment="Top" ClickMode="Release"/>
        <Button x:Name="btn_valider_Copy1" Content="Valider" HorizontalAlignment="Left" Margin="679,256,0,0" Click="OnClick1" VerticalAlignment="Top" ClickMode="Release"/>

    </Grid>
</Window>
